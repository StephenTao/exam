<%@ taglib uri="http://java.sun.com/jsp/jstl/fmt" prefix="fmt"%>
<span ui-sref="choice" class="color-style01" style="width:30%" title="<fmt:message key="question.choice" />">
    <label class="title-icon block-style02 color-style02">CH</label>
    <label class="hand"><fmt:message key="question.choice" /></label>
</span>
<span ui-sref="subjective" class="color-style02" title="<fmt:message key="question.subjective" />">
    <label class="title-icon block-style02 color-style01">SU</label>
    <label class="hand hidden"><fmt:message key="question.subjective" /></label>
</span>
<span ui-sref="complete" class="color-style02" title="<fmt:message key="question.complete" />">
    <label class="title-icon block-style02 color-style01">CO</label>
    <label class="hand hidden"><fmt:message key="question.complete" /></label>
</span>
<span ui-sref="tf" class="color-style02" title="<fmt:message key="question.tf" />">
    <label class="title-icon block-style02 color-style01">TF</label>
    <label class="hand hide"><fmt:message key="question.tf" /></label>
</span>
<span ui-sref="r" class="color-style02" title="<fmt:message key="question.rr" />">
    <label class="title-icon block-style02 color-style01">RR</label>
    <label class="hand hidden"><fmt:message key="question.rr" /></label>
</span>
<span ui-sref="d" class="color-style02" title="<fmt:message key="question.dd" />">
    <label class="title-icon block-style02 color-style01">DD</label>
    <label class="hand hidden"><fmt:message key="question.dd" /></label>
</span>
